@model IEnumerable<Terrenos2.Models.Terreno>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreTerreno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Extencion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DistritoID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropietarioID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreTerreno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Extencion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Precio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DistritoID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PropietarioID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TerrenoID }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Details", "Details", new { id=item.TerrenoID }, new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TerrenoID }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
